<launch>
  <node name="trader" pkg="my_weather_acl" type="trader_node" output="screen">
    <!-- Address of a lighthouse you work on, airalab on sidechain lighthouse by default -->
    <param name="lighthouse" value="0xE85764E29583224C1D063639d2AeeeD7c389DF4d" />
    <!-- A unique model Qm.... -->
    <param name="model" value="Qmd6bn2JGW26hSx7g5gVCmfgB7uigRPrhAukJn77ee3bMM" />
    <!-- A token your CPS works with, bridged XRT token by default -->
    <param name="token" value="0x093ac06910f23570292fd5027a4fA558ed4Cd010" />
    <!-- Ethereum blocks lifetime, ~12-15 sec for one -->
    <param name="order_lifetime" value="100" />
  </node>
</launch>
